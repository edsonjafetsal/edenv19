                    INNER JOIN
                    llx_facturedet fd ON fd.fk_facture = f.rowid
                    INNER JOIN
                    llx_societe s ON f.fk_soc = s.rowid
                    INNER JOIN
                    llx_product p ON fd.fk_product = p.rowid
                    LEFT JOIN
                    llx_categorie_product cats ON cats.fk_product = p.rowid
                    LEFT JOIN
                    llx_categorie cat ON cat.rowid = cats.fk_categorie
                </TableSql>
                <WhereSql>AND f.entity=1
                    AND (f.fk_statut &gt;0 AND f.fk_statut &lt;3 OR f.close_code &lt;&gt; 'replaced')
                </WhereSql>
                <GroupSql>GROUP BY ref , p.rowid , subprice, f.datef, p.label, f.type, fd.qty, s.nom, p.fk_product_type,
                    fd.total_ht, s.code_client, s.rowid
