                $col = $this-&gt;columns[$ct];
                if ( $col-&gt;query_name == &quot;ref&quot; &amp;&amp; preg_match(&quot;/[1]/&quot;, $checks) )
                {
                $col-&gt;attributes[&quot;column_display&quot;] = &quot;hide&quot;;
                }
                if ( $col-&gt;query_name == &quot;datef&quot; &amp;&amp; preg_match(&quot;/[2]/&quot;, $checks) )
                {
                $col-&gt;attributes[&quot;column_display&quot;] = &quot;hide&quot;;
                }
                if ( $col-&gt;query_name == &quot;code_client&quot; &amp;&amp; preg_match(&quot;/[3]/&quot;, $checks) )
                {
                $col-&gt;attributes[&quot;column_display&quot;] = &quot;hide&quot;;
                }
                if ( $col-&gt;query_name == &quot;nom&quot; &amp;&amp; preg_match(&quot;/[4]/&quot;, $checks) )
                {
                $col-&gt;attributes[&quot;column_display&quot;] = &quot;hide&quot;;
