                        p.rowid facid,
                        p.ref ref,
                        t.nom AS customer,
                        p.datec AS date,
                        p.description as description,
                        DATE_FORMAT(p.datec, '%d/%m/%y') AS fecha,
                        t.rowid rowid,
                        p.duree as duree,
                        CASE p.fk_statut
                        WHEN '0'
                        THEN 'Draft'
                        WHEN '1'
                        THEN 'Validated'
                        WHEN '2'
                        THEN 'Billed'
                        ELSE 'Unknown'
                        END AS status
                        FROM
                        llx_fichinter p
                        INNER JOIN
                        llx_societe t ON t.rowid = p.fk_soc
                        WHERE 1 = 1 AND p.entity=1 [ AND p.datec BETWEEN {Date,RANGE1} AND DATE_ADD({Date,RANGE2},
                        INTERVAL '1' DAY )] GROUP BY p.rowid, p.ref, t.nom, p.datec, p.description, t.rowid, p.duree,
                        p.fk_statut
                        ORDER BY p.rowid, p.datec
