    p.rowid AS projectid,
    p.ref,
    p.title,
    CASE p.fk_statut
        WHEN 0 THEN 'Draft'
        WHEN 1 THEN 'Opened'
        WHEN 2 THEN 'Closed'
        ELSE 'Unknow'
    END AS status,
    DATE_FORMAT(p.dateo, '%d/%m/%y') AS date_start,
    DATE_FORMAT(p.datee, '%d/%m/%y') AS date_end,
    s.nom,
    s.rowid AS socid,
    (SELECT 
            SUM(c.total)
        FROM
            llx_facture AS c
        WHERE
            c.fk_projet = p.rowid [AND p.rowid = {Project} ] [AND p.fk_statut IN ({Status})]) AS ingresos,
    (SELECT 
            SUM(c.total_ht)
        FROM
            llx_facture_fourn AS c
        WHERE
            c.fk_projet = p.rowid [AND p.rowid = {Project} ] [AND p.fk_statut IN  ({Status})]) AS gastos,
    (SELECT 
            SUM(tt.thm*(tt.task_duration/3600))
        FROM
            llx_projet_task_time AS tt left join llx_projet_task as t ON tt.fk_task = t.rowid
        WHERE
            t.fk_projet = p.rowid [AND p.rowid = {Project} ] [AND p.fk_statut IN ({Status})]) AS mano_obra,
(SELECT 
            SUM(g.total_ht)
        FROM
            llx_expensereport_det AS g
        WHERE
            g.fk_projet = p.rowid [AND p.rowid = {Project} ] [AND p.fk_statut IN  ({Status})]) AS dietas
FROM
    llx_projet AS p
        LEFT JOIN
    llx_societe AS s ON p.fk_soc = s.rowid
WHERE
    p.entity=1 [AND p.rowid = {Project} ] [AND p.fk_statut IN ({Status})]
ORDER BY p.ref</SQLRaw></SQL><QueryColumns><QueryColumn><Name>projectid</Name><TableName></TableName><ColumnName>p.rowid AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>ref</Name><TableName>p</TableName><ColumnName>ref</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF>25mm</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Ref.</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>title</Name><TableName>p</TableName><ColumnName>title</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Project</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>status</Name><TableName></TableName><ColumnName>CASE p.fk_statut
