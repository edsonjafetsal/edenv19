    s.nom AS societe,
    s.rowid AS socid,
    s.client,
    a.id,
    a.label,
    date_format(a.datep, '%d/%m/%y %H:%i') AS dp,
    date_format(a.datep2, '%d/%m/%y %H:%i')  AS dp2,
    a.fk_user_author,
    a.fk_user_action,
    a.fk_contact,
    a.note,
    a.percent AS percent,
    c.libelle AS type_label,
    sp.lastname,
    sp.firstname,
    au.action, 
    au.autor,
    if(a.percent &lt; 0,'ActionNotApplicable',if(a.percent=0,'ActionsToDoShort',if(a.percent &lt; 100,'ActionRunningShort','ActionsDoneShort'))) as status
FROM
    llx_c_actioncomm AS c,
    llx_actioncomm AS a
        LEFT JOIN
    llx_societe AS s ON a.fk_soc = s.rowid
        LEFT JOIN
    llx_socpeople AS sp ON a.fk_contact = sp.rowid
        LEFT JOIN
    usuarios_agenda au ON a.id = au.evento
WHERE
    c.id = a.fk_action AND a.entity=1 [AND a.datep BETWEEN {Date,RANGE1} AND DATE_ADD({Date,RANGE2}, INTERVAL '1' DAY )]
ORDER BY a.datep, a.datep2 DESC</SQLRaw></SQL><QueryColumns><QueryColumn><Name>societe</Name><TableName></TableName><ColumnName>s.nom AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF>6cm</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Company</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>socid</Name><TableName></TableName><ColumnName>s.rowid AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>client</Name><TableName>s</TableName><ColumnName>client</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>id</Name><TableName>a</TableName><ColumnName>id</ColumnName><ColumnType>char</ColumnTy