                        THEN 'No'
                        WHEN '1'
                        THEN 'Yes'
                        ELSE 'Unknown'
                        END AS
                        paid, p.total_ht total, p.total_tva vat, p.total_ttc total_ttc, t.rowid rowid, date_format(
                        p.datef, '%m' ) AS
                        month, date_format( p.datef, '%Y' ) AS year , p.rowid as facid,IF(t.siren = '',t.tva_intra,t.siren) as siren
                        FROM llx_facture_fourn p
                        INNER JOIN llx_societe t ON t.rowid = p.fk_soc
                        LEFT JOIN llx_categorie_fournisseur cats ON cats.fk_soc = t.rowid
                        LEFT JOIN llx_categorie cat ON cat.rowid = cats.fk_categorie
                        WHERE 1 = 1 AND p.entity=1 [AND p.ref_supplier LIKE '{Ref,false}' ] AND p.fk_statut &gt; 0 GROUP
                        BY p.rowid, p.ref_supplier, t.nom, p.datef, p.paye, p.total_ht, p.total_tva, p.total_ttc,
