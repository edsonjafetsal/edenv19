                    <SQLRaw>SELECT DISTINCT
                        sal.amount AS total_ttc,
                        sal.fk_user fk_user,
                        u.lastname name,
                        u.firstname firstname,
                        sal.year AS year,
                        sal.month AS MONTH,
                        CASE sal.month
                        WHEN '01' THEN 'January'
                        WHEN '02' THEN 'February'
                        WHEN '03' THEN 'March'
                        WHEN '04' THEN 'April'
                        WHEN '05' THEN 'May'
                        WHEN '06' THEN 'June'
                        WHEN '07' THEN 'July'
                        WHEN '08' THEN 'August'
                        WHEN '09' THEN 'September'
                        WHEN '10' THEN 'October'
                        WHEN '11' THEN 'November'
                        WHEN '12' THEN 'December'
                        ELSE 'Unknow'
                        END AS month
                        FROM
                        saldo as sal
                        LEFT JOIN
                        llx_user AS u ON u.rowid = sal.fk_user
                        WHERE
                        1 = 1 [ AND concat(year,MONTH) BETWEEN date_format({Date,RANGE1},'%Y%m') AND
                        date_format({Date,RANGE2},'%Y%m')]

                        ORDER BY sal.fk_user ASC , year ASC , month ASC
