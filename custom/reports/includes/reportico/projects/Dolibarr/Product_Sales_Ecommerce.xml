    p.rowid prodid,
    p.label label,
    SUM(fd.qty) qty,
    p.ref ref,
    p.fk_product_type fk_product_type,
    avg(fd.subprice) as pricemed,
    SUM(fd.total_ht) total,
    p.rowid ecom_product,
    es.site_name
FROM
	llx_facturedet fd
	INNER JOIN llx_facture f on fd.fk_facture=f.rowid
	INNER JOIN llx_product p on fd.fk_product=p.rowid
    INNER JOIN llx_element_element ee on (ee.fk_target=f.rowid and ee.sourcetype = 'commande' AND ee.targettype = 'facture')
    INNER JOIN llx_ecom_order eo ON ee.fk_source = eo.doli_order
	INNER JOIN llx_ecom_site es ON es.rowid = eo.siteid
WHERE f.entity=1
AND (f.fk_statut &gt; 0 AND f.fk_statut &lt; 4)
AND f.type &lt;&gt; 2
GROUP BY fd.fk_product, es.site_name, p.label, p.ref, p.fk_product_type, p.rowid
ORDER BY site_name ASC , p.label ASC</SQLRaw></SQL><QueryColumns><QueryColumn><Name>prodid</Name><TableName></TableName><ColumnName>DISTINCT
