(select pos.note_public  from llx_commande_fournisseur as pos 
	left join llx_element_element as ele on pos.rowid = ele.fk_target and ele.sourcetype like 'commande' and ele.targettype like  'order_supplier'
	where co.rowid = ele.fk_source and pos.fk_statut in (1,2,3,4,5)) AS poll, 
(select posi.note_public  from llx_commande_fournisseur as posi 
	left join llx_element_element as elen on posi.rowid = elen.fk_target and elen.sourcetype like 'commande' and elen.targettype like  'order_supplier'
	where co.rowid = elen.fk_source and posi.fk_statut in (1,2,3,4,5)) AS po_notes,
(select posi.ref  from llx_commande_fournisseur as posi 
	left join llx_element_element as elen on posi.rowid = elen.fk_target and elen.sourcetype like 'commande' and elen.targettype like  'order_supplier'
	where co.rowid = elen.fk_source and posi.fk_statut in (1,2,3,4,5)) AS po, 
cod.subprice AS sell_pri, cod.buy_price_ht AS buy_pri,
co.date_commande AS co_date, cod.qty As qty, cat.label AS categorie,
CONCAT(ROUND(100-(((cod.buy_price_ht*cod.qty)* 100)/cod.total_ht) , 0), '%') AS margin2
FROM  llx_commande AS co
LEFT JOIN llx_commandedet AS cod ON co.rowid = cod.fk_commande
LEFT JOIN llx_commande_extrafields AS coex ON co.rowid = coex.fk_object
LEFT JOIN llx_expeditiondet AS exde ON  cod.rowid = exde.fk_origin_line
LEFT JOIN llx_expedition AS ex ON  exde.fk_expedition = ex.rowid
LEFT JOIN llx_expeditiondet_batch AS eba ON exde.rowid = eba.fk_expeditiondet
LEFT JOIN llx_product AS p ON  cod.fk_product = p.rowid
LEFT JOIN llx_societe AS soc ON co.fk_soc = soc.rowid
  left join
                llx_element_contact as ec ON ec.element_id = co.rowid and ec.fk_c_type_contact in (91)
LEFT JOIN llx_c_type_contact AS ct ON ec.fk_c_type_contact = ct.rowid AND ct.element LIKE 'commande' 
                left join
                llx_user as u ON u.rowid = ec.fk_socpeople
  LEFT JOIN
                    llx_categorie_product cats ON cats.fk_product = p.rowid
                    LEFT JOIN
                    llx_categorie cat ON cat.rowid = cats.fk_categorie
LEFT JOIN dps AS dp ON  co.rowid = dp.id
WHERE 1 = 1  AND p.ref IS NOT NULL 
[AND co.date_commande BETWEEN {date,RANGE1} AND {date,RANGE2} ] 
[AND co.fk_soc IN ({Customer})] 
[AND p.rowid IN ({Product})] 
[AND cat.rowid IN ({Category})] 
[AND ec.fk_socpeople IN ({salesrep})] 
[AND p.fk_product_type IN ({Type})] 
[AND dp.drp IN ({drp})] 
GROUP BY  cod.rowid
ORDER BY co.date_commande</SQLRaw></SQL><QueryColumns><QueryColumn><Name>dps</Name><TableName></TableName><ColumnName>CASE(dp.drp) WHEN '1' THEN 'Yes' ELSE 'No' END AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Dropshiped</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>salesrep</Name><TableName></TableName><ColumnName>CONCAT(u.firstname,' ',u.lastname) AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>extension</Name><TableName></TableName><ColumnName>(cod.buy_price_ht*cod.qty) AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>