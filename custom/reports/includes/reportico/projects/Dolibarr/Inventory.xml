SELECT CONCAT(sm.fk_product,'-',sm.fk_entrepot), SUM(sm.value) AS stock
 FROM llx_stock_mouvement as sm
, llx_entrepot as w
 WHERE w.entity=1
 AND w.rowid = sm.fk_entrepot
[ AND sm.datem &gt;= CONCAT({Date},' 23:59:59') ]
 GROUP BY sm.fk_product, sm.fk_entrepot</SQLText></PreSQL><PreSQL><SQLText>CREATE TEMPORARY TABLE sreal (code varchar(30), mov decimal(24,6))</SQLText></PreSQL><PreSQL><SQLText>INSERT INTO sreal
SELECT CONCAT(ps.fk_product,'-',ps.fk_entrepot), SUM(ps.reel) AS stock
 FROM llx_product_stock as ps
, llx_entrepot as w
 WHERE w.entity=1
 AND w.rowid = ps.fk_entrepot
GROUP BY fk_product, fk_entrepot</SQLText></PreSQL><PreSQL><SQLText>CREATE TEMPORARY TABLE codigo (code varchar(30), prodid int, entid int)</SQLText></PreSQL><PreSQL><SQLText>INSERT INTO codigo
SELECT DISTINCT CONCAT(p.rowid,'-',e.rowid), p.rowid, e.rowid
 FROM llx_product as p, llx_entrepot as e
 WHERE p.entity=1</SQLText></PreSQL></PreSQLS></Query><Assignments><Assignment><AssignName>stock</AssignName><AssignNameNew></AssignNameNew><Expression>{reel} - {mov}</Expression><Condition></Condition></Assignment><Assignment><AssignName>label</AssignName><AssignNameNew></AssignNameNew><Expression>apply_style('PAGE', 'border-style', 'solid');apply_style('PAGE', 'border-width', '1 1 1
