<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Products and services detail - INV</ReportTitle><ReportDescription></ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>.DEFAULT</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>.DEFAULT</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT p.ref AS ref, p.label AS label, p.description AS description, sil.qty AS qty_line, st.ref AS warehouse, FORMAT(sil.subprice, 2) AS up, p.pmp AS pmp1, sil.total_ht AS line_ht1, sil.total_tva AS line_tva1,  un.label AS uom, CASE si.fk_statut
                        WHEN '0'
                        THEN 'Draft'
                        WHEN '1'
                        THEN 'Validated'
                        WHEN '2'
                        THEN 'PaidTotalPartial'
                        WHEN '3'
                        THEN 'Abandoned'
                        ELSE 'Unknown'
                        END AS
                        invoice_status, 
cat.label AS category, cas.label AS parent, soc.nom AS third, soc.address, soc.phone, sil.total_ht AS line_ht, sil.total_tva AS line_tva, si.datef AS date_invoice,
CONCAT(us.firstname,' ',us.lastname) AS autor, si.ref AS invoice, FORMAT(sil.total_ttc, 2) AS totalTax, sil.total_ttc AS totalTaxSum, av.ref AS av, av.total_ttc AS av_total,  
CASE av.fk_statut
                        WHEN '0'
                        THEN 'Draft'
                        WHEN '1'
                        THEN 'Validated'
                        WHEN '2'
                        THEN 'PaidTotalPartial'
                        WHEN '3'
                        THEN 'Abandoned'
                        ELSE 'Unknown'
                        END AS
                        av_status, p.cost_price, p.price,   IFNULL(sil.subprice,'0')-IFNULL(p.cost_price,'0') AS margin, p.rowid AS pid, av.rowid AS avid, si.rowid AS sid
FROM llx_product AS p
LEFT JOIN llx_product_stock AS ps ON  p.rowid = ps.fk_product
LEFT JOIN llx_facturedet AS sil ON p.rowid = sil.fk_product 
LEFT JOIN llx_facture AS si ON  sil.fk_facture = si.rowid 
LEFT JOIN llx_societe_remise_except  AS rc ON rc.fk_facture = si.rowid
LEFT JOIN llx_facture AS av ON av.rowid = rc.fk_facture_source 
LEFT JOIN llx_societe AS soc ON si.fk_soc = soc.rowid 
LEFT JOIN llx_user AS us ON si.fk_user_author = us.rowid 
LEFT JOIN llx_categorie_product AS cap ON p.rowid = cap.fk_product 
LEFT JOIN llx_categorie AS cat ON cap.fk_categorie = cat.rowid 
LEFT JOIN llx_categorie AS cas ON cat.fk_parent = cas.rowid 
LEFT JOIN llx_c_units AS un ON p.fk_unit = un.rowid
LEFT JOIN llx_entrepot AS st ON ps.fk_entrepot = st.rowid
LEFT JOIN llx_element_element AS el ON si.rowid = el.fk_target AND el.targettype LIKE 'invoice_supplier'
 where si.fk_statut &lt;&gt; 0
    AND si.fk_statut &lt;&gt; 3
[AND si.rowid IN ({fa})] 
ORDER BY p.ref ASC</SQLRaw></SQL><QueryColumns><QueryColumn><Name>ref</Name><TableName></TableName><ColumnName>p.ref AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Product Ref.</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>label</Name><TableName></TableName><ColumnName>p.label AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Product Label</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>description</Name><TableName></TableName><ColumnName>p.description AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Product description</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>qty_line</Name><TableName></TableName><ColumnName>sil.q