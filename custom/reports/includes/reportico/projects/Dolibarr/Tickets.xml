CONCAT(u.firstname,' ',u.lastname) as user,
CASE(t.fk_statut) WHEN '0' THEN 'Unread'
WHEN '1' THEN 'Read'
WHEN '2' THEN 'Assigned'
WHEN '3' THEN 'In progess'
WHEN '5' THEN 'Waiting for information'
WHEN '7' THEN 'On hold' 
WHEN '8' THEN 'Closed'
WHEN '9' THEN 'Canceled'
END AS satus
, t.type_code AS type, 
t.category_code AS category, prd.label AS product
FROM llxas_ticket AS t
LEFT JOIN llxas_societe AS soc ON soc.rowid = t.fk_soc 
LEFT JOIN llxas_projet AS proj ON t.fk_project = proj.rowid 
LEFT JOIN llxas_user AS u ON u.rowid = t.fk_user_assign 
LEFT JOIN llxas_ticket_extrafields AS tex ON t.rowid = tex.fk_object
LEFT JOIN llxas_product AS prd ON prd.rowid = tex.product 
WHERE 1 = 1
  [AND t.rowid IN ({ref1})] 
 [AND t.track_id IN ({trackid})] 
 [AND t.fk_soc IN ({soc})] 
 [AND t.fk_user_assign IN ({user1})] 
 [AND tex.productst IN ({produ})] </SQLRaw></SQL><QueryColumns><QueryColumn><Name>ref</Name><TableName></TableName><ColumnName>t.ref AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Ticket Ref.</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>date</Name><TableName></TableName><ColumnName>t.datec AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Creation date</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>track</Name><TableName></TableName><ColumnName>t.track_id AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Track ID</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>third</Name><TableName></TableName><ColumnName>soc.nom AS</Co