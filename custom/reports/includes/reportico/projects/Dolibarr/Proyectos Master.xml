<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Master Projects</ReportTitle><ReportDescription></ReportDescription><PageSize>A4</PageSize><PageOrientation>Portrait</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>            </PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>show</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>.DEFAULT</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT  mo.ref AS mo, 
CASE(mo.status) 
WHEN '0' THEN 'Draft'
WHEN '1' THEN 'Validated'
WHEN '2' THEN 'In Progress'
WHEN '3' THEN 'Produced'
WHEN '9' THEN 'Cancelled' 
END AS mostatus,
rece.ref AS reception, 
CASE(rece.fk_statut) 
WHEN '-1' THEN 'Canceled'
WHEN '0' THEN 'Draft'
WHEN '1' THEN 'Validated'
WHEN '2' THEN 'Processed'
END AS reception_status,
CONCAT(usrec.firstname,' ',usrec.lastname) AS autor_reception, 
env.ref as Envio, 
CASE(env.fk_statut) 
WHEN '-1' THEN 'Canceled'
WHEN '0' THEN 'Draft'
WHEN '1' THEN 'Validated'
WHEN '2' THEN 'Closed'
END AS envstatus,
env.fk_user_author AS envautor, sfo.nom AS Posu,  exp.ref AS Exp,  
com.ref AS OC, com.date_livraison AS ocdeliverydate,
CASE(com.fk_statut) 
WHEN '-1' THEN 'Canceled'
WHEN '0' THEN 'Draft'
WHEN '1' THEN 'Validated'
WHEN '2' THEN 'Order Sent'
WHEN '3' THEN 'Processed' 
END AS ocstatus,
cof.ref AS PO, 
cof.date_livraison AS podeliverydate, 
CASE(cof.fk_statut) 
WHEN '0' THEN 'Draft'
WHEN '1' THEN 'Validated'
WHEN '2' THEN 'Approved'
WHEN '3' THEN 'On Process'
WHEN '4' THEN 'Received/PartiallyShort'
WHEN '5' THEN 'All Received' 
WHEN '6' THEN 'Canceled' 
WHEN '7' THEN 'Not Received' 
WHEN '9' THEN 'Refused' 
END AS postatus,
prop.ref AS CC, 
prop.total AS propamount, 
CASE(prop.fk_statut) 
WHEN '0' THEN 'Draft'
WHEN '1' THEN 'Validated'
WHEN '2' THEN 'Signed'
WHEN '3' THEN 'Not Signed'
WHEN '4' THEN 'Billed' 
END AS propstatus,
p.dateo as inicio, p.ref ref, p.title title, p.dateo fecha, p.rowid, s.label, soc.nom tercero, CONCAT(us.firstname,' ',us.lastname) AS Autor, p.description descripcion, p.dateo fecha_creacion, p.datee fech_ultima 
                        FROM llxas_projet as p                    
LEFT JOIN llxas_c_lead_status as s on p.fk_opp_status=s.rowid and s.active=1
LEFT JOIN llxas_societe as soc on p.fk_soc = soc.rowid 
LEFT JOIN llxas_commande_fournisseur AS cof on p.rowid = cof.fk_projet
LEFT JOIN llxas_element_element AS elcof ON  cof.rowid = elcof.fk_source AND elcof.sourcetype = 'order_supplier' AND elcof.targettype = 'reception'
LEFT JOIN llxas_reception AS rece ON  rece.rowid = elcof.fk_target 
LEFT JOIN llxas_user AS usrec ON usrec.rowid = rece.fk_user_author 
LEFT JOIN llxas_societe as sfo on cof.fk_soc = sfo.rowid
LEFT JOIN llxas_user as us ON  p.fk_user_creat = us.rowid
LEFT JOIN llxas_projet_extrafields as pex ON  p.rowid = pex.fk_object
LEFT JOIN llxas_commande_fournisseur_extrafields AS cox ON  cof.rowid = cox.fk_object
LEFT JOIN llxas_element_contact  AS elc ON p.rowid = elc.element_id
LEFT JOIN llxas_propal AS prop ON p.rowid = prop.fk_projet
LEFT JOIN llxas_commande AS com ON p.rowid = com.fk_projet
LEFT JOIN llxas_commande_extrafields AS comex ON  com.rowid = comex.fk_object
LEFT JOIN llxas_facture  AS fac ON p.rowid = fac.fk_projet	
LEFT JOIN llxas_facture_extrafields AS facex ON fac.rowid = facex.fk_object
LEFT JOIN llxas_facture_fourn  AS fafo ON p.rowid = fafo.fk_projet	
LEFT JOIN llxas_propaldet AS prode ON  prop.rowid = prode.fk_propal
LEFT JOIN llxas_expensereport_det AS expde ON p.rowid = expde.fk_projet 
LEFT JOIN llxas_expensereport AS exp ON expde.fk_expensereport = exp.rowid
LEFT JOIN llxas_supplier_proposal AS spro ON p.rowid = spro.fk_projet
LEFT JOIN llxas_product AS produ ON prode.fk_product = produ.rowid
LEFT JOIN llxas_expedition AS env ON p.rowid = env.fk_projet
LEFT JOIN llxas_paiement_facture AS paifa ON  fac.rowid = paifa.fk_facture
LEFT JOIN llxas_paiementfourn_facturefourn AS paifo ON  fafo.rowid = paifo.fk_facturefourn
LEFT JOIN  llxas_paiement AS paie ON paifa.fk_paiement = paie.rowid
LEFT JOIN  llxas_paiementfourn AS paief ON paifo.fk_facturefourn = paief.rowid
LEFT JOIN llxas_mrp_mo AS mo ON mo.fk_project  = p.rowid
    WHERE 1 = 1 AND p.fk_statut IN  ('1', '2') 
               GROUP BY p.ref;</SQLRaw></SQL><QueryColumns><QueryColumn><Name>mo</Name><TableName></TableName><ColumnName>mo.ref AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>MO Ref</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>mostatus</Name><TableName></TableName><ColumnName>CASE(mo.status) 
