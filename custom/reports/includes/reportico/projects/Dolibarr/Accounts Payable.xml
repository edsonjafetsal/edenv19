                        date_format( datef, '%d/%m/%Y' ) AS fecha, p.paye AS idstatus, CASE p.paye
                        WHEN '0'
                        THEN 'No'
                        WHEN '1'
                        THEN 'Yes'
                        ELSE 'Unknown'
                        END AS
                        paid, ROUND(p.total_ttc,2) total, ROUND(p.tva,2) vat, ROUND(p.total_ttc,2) total_ttc,  ROUND(p.total_ttc,2) total2, ROUND(p.tva,2) vat2, ROUND(p.total_ttc,2) total_ttc2, t.rowid rowid,
                        date_format( p.datef, '%m' ) AS
                        month, date_format( p.datef, '%Y' ) AS year, IF(t.siren = '',t.tva_intra,t.siren) as siren,
                        p.date_lim_reglement

                        FROM llx_facture_fourn p
                        INNER JOIN llx_societe t ON t.rowid = p.fk_soc
                        LEFT JOIN llx_societe_commerciaux sc ON t.rowid=sc.fk_soc
                        LEFT JOIN llx_user u on sc.fk_user=u.rowid
                        LEFT JOIN llx_categorie_societe cats ON cats.fk_soc = t.rowid
                        LEFT JOIN llx_categorie cat ON cat.rowid = cats.fk_categorie
                        WHERE 1 = 1 AND  p.type in (0,1,2) AND p.fk_statut=1 AND p.paye=0 AND p.entity=1 --  [AND p.ref LIKE '{Ref,false}' ]
                        GROUP BY p.rowid , p.ref, t.nom,
                        p.datef, p.paye, p.total_ttc, p.tva, p.total_ttc, t.rowid, p.datef, p.datef, t.siren, t.tva_intra
                        ORDER BY t.nom
