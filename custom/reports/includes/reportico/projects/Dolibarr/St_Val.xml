SELECT pbc.batch AS batc, (CASE pbc.batch WHEN NULL THEN '0' ELSE '1' END) AS ba
FROM llx_product AS p
LEFT JOIN llx_product_stock AS ps ON p.rowid = ps.fk_product 
LEFT JOIN llx_entrepot AS w ON ps.fk_entrepot = w.rowid 
LEFT JOIN llx_product_fournisseur_price  AS pf ON p.rowid = pf.fk_product 
LEFT JOIN llx_product_batch AS pbc ON ps.rowid = pbc.fk_product_stock 
WHERE  pbc.batch IS NOT NULL  AND ps.reel NOT IN (0); </SQLText></PreSQL><PreSQL><SQLText>CREATE TEMPORARY TABLE noba (pid INT(11), qty INT(12), wid INT(12))</SQLText></PreSQL><PreSQL><SQLText>INSERT INTO noba (pid, qty, wid)
SELECT  p.rowid AS pid, ps.reel AS qty, w.rowid  AS wid
FROM llx_product AS p
LEFT JOIN llx_product_stock AS ps ON p.rowid = ps.fk_product 
LEFT JOIN llx_entrepot AS w ON ps.fk_entrepot = w.rowid 
LEFT JOIN llx_product_fournisseur_price  AS pf ON p.rowid = pf.fk_product 
LEFT JOIN llx_product_batch AS pbc ON ps.rowid = pbc.fk_product_stock 
WHERE  pbc.batch IS NULL  AND ps.reel NOT IN (0); </SQLText></PreSQL></PreSQLS></Query><Assignments><Assignment><AssignName>cost_price_value2</AssignName><AssignNameNew></AssignNameNew><Expression>number_format({cost_price_value2},2,'.',',')</Expression><Condition></Condition></Assignment><Assignment><AssignName>product</AssignName><AssignNameNew></AssignNameNew><Expression>apply_style('PAGE', 'border-style', 'solid');apply_style('PAGE', 'border-width', '1 1 1 1');apply_style('PAGE', 'border-color', '#222222');apply_style('PAGE', 'background-color', '#f3fff3');</Expression><Condition></Condition></Assignment><Assignment><AssignName>product</AssignName><AssignNameNew></AssignNameNew><Expression>apply_style('ROW', 'background-color', '#e0e0ff');</Expression><Condition></Condition></Assignment><Assignment><AssignName>product</AssignName><AssignNameNew></AssignNameNew><Expression> apply_style('ROW', 'background-color', '#eeeeff')</Expression><Condition>( lineno() % 2 == 1 ) </Condition></Assignment><Assignment><AssignName>sumltqt</AssignName><AssignNameNew></AssignNameNew><Expression>sum({lotqty},{warehouse})</Expression><Condition></Condition></Assignment><Assignment><AssignName>sumltt</AssignName><AssignNameNew></AssignNameNew><Expression>sum({lotqty})</Expression><Condition></Condition></Assignment><Assignment><AssignName>sumcop</AssignName><AssignNameNew></AssignNameNew><Expression>sum({cost_price_value},{warehouse})</Expression><Condition></Condition></Assignment><Assignment><AssignName>sumcoto</AssignName><AssignNameNew></AssignNameNew><Expression>sum({cost_price_value})</Expression><Condition></Condition></Assignment><Assignment><AssignName>sumcoto</AssignName><AssignNameNew></AssignNameNew><Expression>number_format({sumcoto},2,'.',',')</Expression><Condition></Condition></Assignment><Assignment><AssignName>sumcop</AssignName><AssignNameNew></AssignNameNew><Expression>number_format({sumcop},2,'.',',')</Expression><Condition></Condition></Assignment><Assignment><A